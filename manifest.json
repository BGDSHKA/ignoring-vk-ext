{
    "manifest_version": 2,
    "name": "igoring-vk-ext",
    "version": "0.7.1",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": [
            "https://vk.com/im*"
          ],
          "js": ["observer.js"]
        }
      ],
      "browser_action": {
        "default_icon": "icon.png"
      }

}